{
  "compilerOptions": {
    // "baseUrl": "./src",
    "target": "ES5",
    "lib": ["dom", "ES2016"],
    "outDir": "dist",
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowJs": true,
    "paths": {
      "@components/*": ["./src/components/*"],
      "@assets/*": ["./src/assets/*"],
      "@pages/*": ["./src/pages/*"],
      "@providers/*": ["./src/providers/*"],
      "@constants/*": ["./src/constants/*"],
      "@interfaces/*": ["./src/interfaces/*"],
      "@redux/*": ["./src/redux/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@api/*": ["./src/api/*"],
      "@utils/*": ["./src/utils/*"],
      "@generated/*": ["./src/generated/*"],
      "@routes/*": ["./src/routes/*"],
      "ui/*": ["./@mf-types/ui/*"]
    },
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "isolatedModules": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
